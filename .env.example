# .env.example
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=postgresql://solsol:solsol_password@localhost:5432/solsol
REDIS_URL=redis://localhost:6379

# Shyft API
SHYFT_API_KEY=your_api_key_here
SHYFT_GRPC_ENDPOINT=https://grpc.ams.shyft.to
SHYFT_GRPC_TOKEN=your_grpc_xtoken

# Jupiter API (v3)
JUPITER_API_URL=https://lite-api.jup.ag/price/v3

# Solana RPC (backup)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Application
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# Worker Threads
GRPC_WORKER_COUNT=2
