{
  "name": "solsol",
  "version": "1.0.0",
  "description": "Solana Memecoin Data Collection System",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
