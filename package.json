    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
